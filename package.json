{
  "name": "generator-mavention-spa",
  "version": "1.0.0",
  "description": "Yeoman Mavention generator to scaffold a SPA with Angular 1 written in ES6 (Babel), TypeScript through Webpack including tools Gulp 4, ESLint, Browsersync and Karma",
  "homepage": "http://fountainjs.io/",
  "repository": "mavention/generator-mavention-spa",
  "author": "Mavention",
  "contributors": [
    "Robert Jaakke <robert.jaakke@mavention.nl> (http://www.mavention.nl/)",
    "Albert-Jan Schot Jaakke <albert.jan.schot@mavention.nl> (http://www.mavention.nl/)"
  ],
  "main": "index.js",
  "keywords": [
    "yeoman-generator",
    "yeoman",
    "generator",
    "gulp",
    "gulpfile",
    "fountain",
    "cli",
    "angular"
  ],
  "dependencies": {
    "chalk": "^1.1.3",
    "ejs": "^2.5.6",
    "falafel": "^2.0.0",
    "lodash": "^4.17.4",
    "mem-fs": "^1.1.2",
    "mem-fs-editor": "^3.0.2",
    "minimatch": "^3.0.3",
    "yeoman-generator": "^1.1.0",
    "generator-fountain-gulp": "^1.0.0",
    "generator-fountain-webpack": "mavention/generator-fountain-webpack"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-eslint": "^7.1.1",
    "cartesian-product": "^2.1.2",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "chai-things": "^0.2.0",
    "cross-spawn": "^5.0.1",
    "eslint": "^3.15.0",
    "eslint-config-xo-space": "^0.15.0",
    "eslint-plugin-babel": "^4.0.1",
    "fs-extra": "^2.0.0",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-eslint": "^3.0.1",
    "gulp-exclude-gitignore": "^1.1.1",
    "gulp-nsp": "^2.4.2",
    "nyc": "^10.1.2"
  },
  "ava": {
    "files": [
      "test/lib/**/*.js"
    ]
  },
  "nyc": {
    "include": [
      "lib/*.js",
      "generators/**/*.js"
    ],
    "exclude": [
      "lib/techs.json",
      "lib/test-utils.js",
      "generators/**/templates/**"
    ]
  },
  "scripts": {
    "test": "gulp",
    "prepublish": "gulp prepublish"
  },
  "license": "MIT",
  "eslintConfig": {
    "extends": "xo-space/esnext"
  }
}
